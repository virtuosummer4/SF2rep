function A = tempfnc(X)

N = 8;
stp = 17;
Xq = quantise(X, stp);
ref = std(X(:)-Xq(:));
bitref = getbits(Xq);



    fun1 = @(X, s) lbtfw(X, N, s);
    fun2 = @(Yq, s) lbtbw(Yq, N, s);
    
    fun = @(step, s) qntest(fun1, fun2, X, step);
    
    opt(i) = golden(fun, ref);
    Y = lbtfw(X, N, s(i));
    Y = quantise(Y, opt(i));
    compr(i) = bitref/dctbpp(regroup(Y,N),N);


end   